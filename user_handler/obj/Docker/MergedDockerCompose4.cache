services:
  notification_handler:
    build:
      context: /Users/user/Projects/RPI/user_handler
      dockerfile: ../notification_handler/notification_handler/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: notification_handler
      target: base
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44355'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: notificationhandler:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "bin/Debug/netcoreapp3.1/notification_handler.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 39748
      target: 80
    - published: 44355
      target: 443
    tty: true
    volumes:
    - /Users/user/Projects/RPI/notification_handler/notification_handler:/app:rw
    - /Users/user/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/user/.aspnet/https:/root/.aspnet/https:ro
    - /Users/user/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/user/.nuget/packages:/root/.nuget/packages:ro
    - /Users/user/Projects/RPI/user_handler:/src:rw
  user_handler:
    build:
      context: /Users/user/Projects/RPI/user_handler
      dockerfile: user_handler/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: user_handler
      target: build
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTPS_PORT: '44305'
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: userhandler:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "bin/Debug/netcoreapp3.1/user_handler.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 57500
      target: 80
    - published: 44305
      target: 443
    tty: true
    volumes:
    - /Users/user/Projects/RPI/user_handler/user_handler:/app:rw
    - /Users/user/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/user/.aspnet/https:/root/.aspnet/https:ro
    - /Users/user/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/user/.nuget/packages:/root/.nuget/packages:ro
    - /Users/user/Projects/RPI/user_handler:/src:rw
version: '3.4'